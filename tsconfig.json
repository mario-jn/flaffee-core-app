{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "src",
    "outDir": "dist",
    "moduleResolution": "node",
//    "allowJs": true,
//    "allowImportingTsExtensions": true
  },
  "include": ["src/**/*"],
  "exclude": ["test"]
}
